<h1 class="text-danger">Galeria de Imagens</h1>
    {{#if images}}
        {{#if images.length}}
            {{#each images}}
            <img src="{{this}}" alt="{{this}}" style="width:200px;height:auto;">
            {{/each}}
        {{else}}
            <p>Nenhuma imagem encontrada.</p>
        {{/if}}
        {{else}}
        <p>Nenhuma imagem encontrada.</p>
    {{/if}}